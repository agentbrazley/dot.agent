{
  "name": "agent-frontmatter",
  "version": "0.1.0",
  "description": "Agent file frontmatter parser, validator, and discovery system",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "agent-lint": "./dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "test": "jest",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "npm run build"
  },
  "keywords": [
