  "author": "Nik Brazley",
  "license": "MIT",
  "dependencies": {
    "gray-matter": "^4.0.3",
    "js-yaml": "^4.1.0",
    "zod": "^3.23.8",
    "semver": "^7.6.3",
    "commander": "^12.1.0",
    "chalk": "^5.3.0",
    "ora": "^8.1.0",
    "meilisearch": "^0.41.0",
    "glob": "^11.0.0"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.0.0",
    "@types/semver": "^7.5.8",
    "typescript": "^5.5.3",
    "tsx": "^4.16.2",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.12",
    "ts-jest": "^29.2.3",
    "prettier": "^3.3.2"
